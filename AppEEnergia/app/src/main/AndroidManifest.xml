<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.casasolarctpi.appeenergia">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_innovatec"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_innovatec"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controllers.RestaurarContrasenaActivity"></activity>
        <activity android:name=".controllers.RegistroActivity" />
        <activity android:name=".controllers.LoginActivity" />
        <activity
            android:name=".controllers.MenuActivity"
            android:configChanges="screenSize|orientation|keyboardHidden"
            android:label="@string/title_activity_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".controllers.Splash"
            android:configChanges="screenSize|orientation|keyboardHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>